    .scope S_0x556e80b48510;
T_1 ;
    %wait E_0x556e80b48900;
    %load/vec4 v0x556e80b6ea80_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x556e80b6eb40_0, 0, 8;
T_1.0 ;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x556e80b48390;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556e80b6ed50_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556e80b6ec80_0, 0, 1;
    %end;
