    .scope S_0x556e80b48390;
T_3 ;
    %delay 17, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x556e80b6ed50_0, 0, 1;
    %delay 11, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556e80b6ed50_0, 0, 1;
    %delay 29, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x556e80b6ed50_0, 0, 1;
    %delay 11, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556e80b6ed50_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 11 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x556e80b48390;
T_4 ;
    %delay 5, 0;
    %load/vec4 v0x556e80b6ec80_0;
    %nor/r;
    %store/vec4 v0x556e80b6ec80_0, 0, 1;
