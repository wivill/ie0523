v0x556e80b6eb40_0 .var "out", 7 0;
E_0x556e80b48900 .event edge, v0x556e80b6ea80_0;
E_0x556e80b4a1f0 .event posedge, v0x556e80b12180_0;
    .scope S_0x556e80b48510;
T_0 ;
    %wait E_0x556e80b4a1f0;
    %load/vec4 v0x556e80b6eb40_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x556e80b6eb40_0, 0;
    %jmp T_0;
